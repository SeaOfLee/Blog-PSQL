  <div class='row'>
    <h1 class='text-center'>Log In!</h1>
    <div class="login col-md-6 col-md-offset-3">
      <%= bootstrap_form_for @user, url:{ :controller => 'sessions', :action => "create"} do |f| %>
      <%= f.text_field :email %><br>
      <%= f.password_field :password %><br>
      <%= f.submit "Log In", class: "btn btn-primary col-md-12" %>
      <br><br>
      <% end %>
      <br><br>
      <%= button_to('Sign Up', new_user_path, method: :get, :class => 'col-md-12 btn btn-large btn-default')  %>
    </div>
  </div>
  <br><br>