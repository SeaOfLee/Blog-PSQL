<%= bootstrap_form_for(@user) do |f| %>
  <div class='row'>
    <div class='col-md-6 col-md-offset-3'>
      <%= bootstrap_form_for(@user) do |f| %>
      <%= f.email_field :email %>
      <%= f.text_field :nickname %>
      <%= f.password_field :password %>
      <%= f.password_field :password_confirmation %>
      <%= f.submit "Sign Up", class: "btn btn-primary col-md-12" %>
      <% end %>
  </div>
</div>
<% end %>

